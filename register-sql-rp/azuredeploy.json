{
    "$schema": "https://schema.management.azure.com/schemas/2019-08-01/managementGroupDeploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "mgtGroup": {
        "type": "string",
        "defaultValue": "mgtgroupId",
        "metadata": {
          "description": "Management group where identity is scoped."
        }
      },
      "subIDforFunction": {
          "type": "string"
      },
      "location": {
          "type": "string",
          "defaultValue": "eastus"
      },
      "resourceGroupName": {
          "type": "string"
      }
    },
    "functions": [],
    "variables": {},
    "resources": [

        {
            "name": "createRole",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-10-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/edm-ms/poc/main/register-sql-rp/sql-rp-role.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "mgtGroup": {
                        "value": "[parameters('mgtGroup')]"
                    }
                }
            }
        },
        {
            "name": "createFunction",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2020-10-01",
            "subscriptionId": "[parameters('subIDforFunction')]",
            "dependsOn": ["[resourceId('Microsoft.Resources/deployments', 'createRole')]"],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/edm-ms/poc/main/register-sql-rp/azure-function.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "location": {
                        "value": "[parameters('location')]"
                    },
                    "resourceGroupName": {
                        "value": "[parameters('resourceGroupName')]"
                    }
                }
            }
        }

    ],
    "outputs": {}
}